MilestoneDto:
  type: object
  properties:
    milestoneId:
      type: integer
      format: int64
    proposalId:
      type: integer
      format: int64
    title:
      type: string
    budget:
      type: number
      format: double
    deadline:
      type: string
      format: date-time
    status:
      type: string
      enum: [PENDING, TERMINATED, IN_PROGRESS, FINISHED]
    isVisible:
      type: boolean
    createdAt:
      type: string
      format: date-time
    updatedAt:
      type: string
      format: date-time